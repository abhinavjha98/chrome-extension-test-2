{
    "manifest_version":2,
    "name":"File blocker",
    "version":"0.0.1",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["Content.js"]
        }
    ],
    "permissions": [
        "http://40.75.0.156/*",
        "webRequest", 
        "webRequestBlocking", 
        "<all_urls>",
        "tabs"
    ]
}